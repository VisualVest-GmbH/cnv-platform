{
  "name": "@cnv-platform/client",
  "version": "0.0.8",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=local parcel index.html --host localhost",
    "start:prod": "NODE_ENV=production parcel index.html --host localhost",
    "build:local": "NODE_ENV=local && parcel build index.html --public-url ./ --no-source-maps ",
    "build:prod": "NODE_ENV=production && parcel build index.html chat.scss --public-url ./ --no-source-maps --no-scope-hoist --no-content-hash --no-cache"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.19.26",
    "@fseehawer/react-circular-slider": "^2.3.5",
    "@material-ui/core": "^4.0.0",
    "@toycode/markdown-it-class": "^1.2.4",
    "@types/react": "^16.8.6 || ^17.0.0",
    "array-move": "^3.0.1",
    "axios": "^0.21.1",
    "browserslist": "^4.21.3",
    "classnames": "^2.2.6",
    "components": "^0.1.0",
    "date-fns": "^2.28.0",
    "global": "^4.4.0",
    "html-react-parser": "^0.14.2",
    "lottie-web": "^5.9.6",
    "markdown-it": "^12.0.3",
    "markdown-it-link-attributes": "^2.1.0",
    "markdown-it-sanitizer": "^0.4.3",
    "markdown-it-sup": "^1.0.0",
    "postcss-modules": "^4.3.1",
    "prop-types": "^15.7.2",
    "rc-slider": "^9.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-player": "^2.7.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-sortable-hoc": "^2.0.0",
    "redux": "^4.0.5",
    "rxjs": "^7.5.7",
    "smoothscroll-polyfill": "^0.4.4",
    "socket.io-client": "^4.5.1",
    "source-sans": "^3.28.0",
    "source-sans-pro": "^3.6.0",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "buffer": "^5.5.0",
    "parcel": "^2.7.0",
    "parcel-namer-custom": "^0.2.0",
    "parcel-plugin-url-loader": "^1.3.1",
    "parcel-resolver-ignore": "^2.1.3",
    "punycode": "^1.4.1",
    "sass": "^1.28.0",
    "typescript": "^4.5.5"
  },
  "alias": {
    "ReactDOM": "react-dom",
    "ReactRedux": "react-redux",
    "React": "react"
  },
  "parcel-namer-custom": {
    ".html?$": "[name].[type]",
    ".js?$": "[name].[type]",
    ".css?$": "[name].[type]"
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "targets": {
    "default": {
      "distDir": "../../public/app"
    }
  }
}
